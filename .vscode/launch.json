{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "(gdb) Launch main.c",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceRoot}/a2/dijkstra.out",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceRoot}",
        "environment": [],
        "externalConsole": true,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      }
    ]
  }